@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}
<table class="table table-dark table-striped">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Price</th>
        </tr>
    </thead>
    <tbody>
    @for (int i = 0; i < Model.products.Count; i++)
    {
        <tr>
            <th scope="row">@(i+1)</th>
            <td>@Model.products[i].Name</td>
            <td>@Model.products[i].Price</td>
        </tr>
    }
    </tbody>
</table>
